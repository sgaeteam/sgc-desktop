SELECT 1
USE CCONV
INDEX ON MATRICULA TO Y

SELECT 2
USE SOCIOS
INDEX ON MATRICULA TO X

DBGOTOP()
WHILE !SOCIOS->(EOF())
  IF !CCONV->(DBSEEK(SOCIOS->MATRICULA))
     CCONV->(DBAPPEND())
     REPLACE CCONV->MATRICULA WITH SOCIOS->MATRICULA ,;
             CCONV->QCONMES   WITH 2,;
             CCONV->QDISP     WITH 2
  ENDIF
  SOCIOS->(DBSKIP())
ENDDO
QUIT
